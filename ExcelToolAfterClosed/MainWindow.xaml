        <Label Content="Wait..." Height="20px" FontSize="10px" HorizontalAlignment="Left" VerticalAlignment="Top" VerticalContentAlignment="Center"></Label>
        <ProgressBar x:Name="pb_process" Height="20"></ProgressBar>
    </StackPanel>
</Window>
